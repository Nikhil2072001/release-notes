{
  "name": "feedthemonsterjs",
  "version": "1.3.0",
  "description": "education app",
  "main": "index.js",
  "scripts": {
    "release": "standard-version --release-as minor",
    "dev": "webpack --config webpack.config.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Nikhil2072001/release-notes.git"
  },
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@release-it/conventional-changelog": "^8.0.1",
    "node-fetch": "^3.3.1",
    "path": "^0.12.7",
    "release-it": "^17.4.0",
    "simple-git": "^3.19.1"
  },
  "release-it": {
    "git": {
      "commitMessage": "chore: release v${version}"
    },
    "github": {
      "release": true
    },
    "npm": {
      "publish": false
    },
    "plugins": {
      "@release-it/conventional-changelog": {
        "infile": "CHANGELOG.md",
        "preset": {
          "name": "conventionalcommits",
          "types": [
            {
              "type": "feat",
              "section": "Features"
            },
            {
              "type": "fix",
              "section": "Bug Fixes"
            },
            {
              "type": "cont",
              "section": "Content Changes"
            },
            {
              "type": "other",
              "section": "Other Minor Changes"
            },
            {}
          ]
        }
      }
    }
  },
  "devDependencies": {
    "standard-version": "^9.5.0"
  }
}
